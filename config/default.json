{
    "templates": [
        {
          "source": "templates/haproxy.cnf.njk",
          "path": "dump",
            "filename": "haproxy.cfg",
          "command": "haproxy -D -p /var/run/haproxy.pid  -f /etc/haproxy/haproxy.cfg -sf $(cat /var/run/haproxy.pid) || true"
        },
        {
            "source": "templates/certificates.sh.njk",
            "path": "dump/",
            "filename": "certificates.sh",
            "command": "sh /etc/haproxy/certificates.sh"
        }
    ]
}
